{{log model}}
{{#each model as |subcommittee|}}
  <h2>Details for <a href="{{subcommittee.url}}" target="_blank">{{subcommittee.name}}</a></h2>
  <div class="row">
    <div class="col s6">
      <div class="collection with-header">
        <div class="collection-header"><h4>Members:</h4></div>
        {{#each subcommittee.members as |member|}}
        <a href="{{member.legislator.website}}" class="collection-item">{{member.legislator.first_name}} {{member.legislator.last_name}}</a>
        {{/each}}
      </div>
    </div>
    <div class="col s6">

    </div>
  </div>
{{/each}}
